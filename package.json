{
  "name": "github-explorer",
  "version": "1.0.0",
  "author": "Bojana Drangova",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:cache": "jest --clearCache",
    "test:watch": "yarn test --watchAll",
    "cy:open": "cypress open",
    "test:e2e:dev": "npm-run-all --parallel --race start cy:open",
    "test:e2e": "yarn build && npm-run-all --parallel --race serve:build cy:run",
    "start": "webpack-dev-server --config config/webpack.config.js --env.env=dev --open",
    "build": "webpack --config config/webpack.config.js --env.env=prod",
    "build:analyze": "yarn build --env.analyze=true",
    "deploy:firebase": "yarn build && firebase deploy",
    "serve:build": "serve -s build --port 8000"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "firebase": "^7.14.5",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "parse-link-header": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.6.4",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/plugin-transform-react-inline-elements": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "cypress": "^4.7.0",
    "dotenv-webpack": "^1.8.0",
    "enhanced-resolve": "^4.1.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^0.9.0",
